{
  "name": "solidarity-platform",
  "version": "2.41.0",
  "description": "Solidarity Platform implementing the Henry 7 Step 14 Trott Waltz numerological system - Professional Bridging Anchor Processing & Financial Commerce Platform",
  "main": "launcher.js",
  "scripts": {
    "start": "node src/api/server.js",
    "dev": "nodemon src/api/server.js --watch src",
    "test-server": "node backend-test-server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:security": "node security/security-tests.js",
    "test:colors": "node test_color_utils.js",
    "test:bridging": "python3 bridging_anchor_systems/bridging_anchor_processor.py",
    "test:advanced": "python3 bridging_anchor_systems/bridging_anchor_test_suite.py",
    "test:quantum": "python3 quantum_cubic_backend.py report",
    "quantum:demo": "node launcher.js quantum",
    "quantum:cubic": "node launcher.js cubic",
    "quantum:bigask": "node launcher.js bigask",
    "test:api": "jest --testPathPattern=tests/api",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "build": "npm run build:frontend && npm run build:docs",
    "build:frontend": "cd frontend && npm run build",
    "build:docs": "node scripts/generateDocs.js",
    "setup": "node scripts/setup.js",
    "setup:dev": "node scripts/setupDev.js",
    "setup:prod": "node scripts/setupProd.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "deploy": "node scripts/deploy.js",
    "sync:coul-curve": "node scripts/syncCoulCurve.js",
    "link:coul-curve": "node scripts/linkRepositories.js",
    "test:cross-repo": "npm test && cd ../Coul-Curve && npm test",
    "deploy:staging": "NODE_ENV=staging node scripts/deploy.js",
    "deploy:production": "NODE_ENV=production node scripts/deploy.js",
    "lint": "eslint src/ --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "validate": "npm run lint && npm run test && npm run build",
    "health": "node scripts/healthCheck.js",
    "monitor": "node scripts/monitor.js",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "clean": "rimraf node_modules/ frontend/node_modules/ frontend/build/ coverage/ logs/",
    "reset": "npm run clean && npm install",
    "docker:build": "docker build -t solidarity-platform .",
    "docker:run": "docker run -p 3000:3000 --env-file .env solidarity-platform",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "security:test": "node security/security-tests.js",
    "performance:test": "node scripts/performanceTest.js",
    "load:test": "node scripts/loadTest.js",
    "stress:test": "node scripts/stressTest.js",
    "docs:serve": "node scripts/serveDocs.js",
    "api:docs": "node scripts/generateApiDocs.js",
    "frontend:install": "cd frontend && npm install",
    "frontend:dev": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "frontend:test": "cd frontend && npm test",
    "office:start": "node start-office-suite.js",
    "office:demo": "node demo-office-client.js",
    "office:setup": "node mock-office-suite-manager.js",
    "hidden:init": "node mock-office-suite-manager.js",
    "hidden:start": "node start-office-suite.js",
    "hidden:demo": "node demo-office-client.js",
    "hidden:test": "node demo-office-client.js && echo 'Hidden system test complete'",
    "correction:demo": "node src/correctedSolidaritySystem.js",
    "correction:test": "node src/focusedPassCorrector.js",
    "solidarity:full": "node src/correctedSolidaritySystem.js",
    "anchor:demo": "node src/anchorBridgingSystem.js",
    "anchor:test": "node src/anchorBridgingSystem.js",
    "anchor:full": "node src/anchorBridgingSystem.js",
    "master:anchor": "node src/anchorBridgingSystem.js",
    "ultimate:demo": "node src/ultimateSolidaritySystem.js",
    "ultimate:test": "node src/ultimateSolidaritySystem.js",
    "ultimate:full": "node src/ultimateSolidaritySystem.js",
    "solidarity:ultimate": "node src/ultimateSolidaritySystem.js"
  },
  "keywords": [
    "bridging-anchor-processing",
    "signal-processing",
    "financial-commerce",
    "blockchain",
    "spatial-processing",
    "professional-studio",
    "base-ratio-analysis",
    "real-time-processing",
    "secure-transactions",
    "api-server",
    "frontend-ready",
    "solidarity",
    "numerology",
    "sacred-geometry",
    "henry-7-step-14-trott-waltz",
    "cubit-calculation",
    "bridging-progression"
  ],
